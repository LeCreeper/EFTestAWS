@model IEnumerable<EFTestAWS.Models.PageData>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PageTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PageDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ending)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Result)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Victory)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PageTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PageDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ending)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Result)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Victory)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PageID }) |
            @Html.ActionLink("Details", "Details", new { id=item.PageID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PageID })
        </td>
    </tr>
}

</table>
